{
  "name": "advance-student-portal",
  "version": "0.0.1",
  "scripts": {
    "dev:only": "svelte-kit dev --host",
    "build:only": "svelte-kit build",
    "preview": "svelte-kit preview",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/tailwind-output.css",
    "dev": "concurrently \"yarn run dev:only\" \"yarn run tailwind:watch\"",
    "build": "yarn run tailwind:build && yarn run build:only"
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "postcss-cli": "^9.0.2",
    "svelte": "^3.34.0",
    "tailwindcss": "^2.2.19"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "@panzoom/panzoom": "^4.4.3",
    "@popperjs/core": "^2.10.2",
    "@sentry/browser": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "@sveltejs/adapter-vercel": "^1.0.0-next.31",
    "blueimp-md5": "^2.19.0",
    "cookie": "^0.4.1",
    "dayjs": "^1.10.7",
    "debounce": "^1.2.1",
    "normalizr": "^3.6.1",
    "parse-srt": "^1.0.0-alpha",
    "svelte-slider": "^1.0.1",
    "tippy": "^0.0.0"
  }
}
